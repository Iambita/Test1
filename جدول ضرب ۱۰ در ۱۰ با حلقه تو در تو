#include <stdio.h>

int main() {
    int i, j;

    // حلقه‌ی خارجی برای ردیف‌های جدول
    for (i = 1; i <= 10; i++) {
        // حلقه‌ی داخلی برای ستون‌های جدول
        for (j = 1; j <= 10; j++) {
            printf("%4d", i * j); // چاپ حاصل‌ضرب i و j
        }
        printf("\n"); // چاپ یک خط جدید بعد از هر ردیف
    }

    return 0;
}
